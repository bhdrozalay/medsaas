    "db:studio": "prisma studio",
    "db:seed": "tsx src/seed.ts",
    "db:setup-all": "tsx scripts/setup-all-databases.ts",
    "db:test": "tsx src/test.ts",
    "db:seed-all": "tsx scripts/seed-all-databases.ts"
  },
  "devDependencies": {
    "typescript": "^5.2.2",
    "tsx": "^4.7.0",
    "prisma": "^5.7.0"
  },
  "dependencies": {
    "@prisma/client": "^5.7.0",
    "zod": "^3.22.0",
    "bcryptjs": "^2.4.3",
    "nanoid": "^5.0.0"
